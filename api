url = gopiko.fr
port: 8080
register:
	desc: create user
	url: /register
	method: POST
	fields:
		username, password, email, firstname, lastname
login:
	desc: log user
	url: /login
	method: post
	field: 
		username, password
logout:
	desc: logout user
	url: /logout
	method: get

connected:
	desc: get user is connected bool
	url: /connected
	method: get

profile:
	desc: get user info
	url: /profile
	method: get

profile:
	desc: update profile
	url: /profile/:id
	method: post
	id: {{"username",...}}

